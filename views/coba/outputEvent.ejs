<div class="container">
  <div class="row">
      <div class="col mt-5">
          <h1 class="text-center">PERCOBAAN OUTPUT TABEL EVENTS</h1><br>
          <!-- <a href="/create" class="btn btn-primary">Tambah Data</a> -->
          <table class="table">
              <thead>
                <tr class="text-center">
                  <th scope="col">ID</th>
                  <th scope="col">Title</th>
                  <th scope="col">Date Start</th>
                  <th scope="col">Time Start</th>
                  <th scope="col">Price</th>
                  <!-- <th scope="col" colspan="3"></th> -->
                </tr>
              </thead>                
              <tbody>
              <% data.forEach(i => { %>
                <tr class="text-center">
                  <td><%= i.id %></td>
                  <td><%= i.title %></td>
                  <td><%= i.dateStart%></td>
                  <td><%= i.timeStart %></td>
                  <td><%= i.price %></td>
                  <!-- <td><a href="/history/<%= i.id %>"><i class="fas fa-history"></i></a></td>
                  <td><a href="/update/<%= i.id %>"><i class="fas fa-edit"></i></a></td>
                  <td><a href="/delete/<%= i.id %>"><i class="fas fa-trash-alt"></i></a></td> -->
                </tr>
              <% }) %>
              </tbody>
          </table>
      </div>
  </div>
</div>